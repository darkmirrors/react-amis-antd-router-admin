{
  "name": "amis-react-starter",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:fix": "eslint src/**/*.js --fix",
    "commit": "cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-configrc.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'git commit trigger husky pre-commit hook'",
      "commit-msg": "echo 'git commit trigger husky commit-msg hook'",
      "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --cache --fix",
      "git add"
    ],
    "src/**": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@types/node": "^16.11.11",
    "@types/react-dom": "^18.0.1",
    "@vitejs/plugin-react": "^1.1.0",
    "amis": "^1.7.0",
    "antd": "^4.19.5",
    "autoprefixer": "^10.4.4",
    "axios": "^0.26.1",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "copy-to-clipboard": "^3.3.1",
    "css-loader": "^6.5.1",
    "cz-customizable": "^6.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-alloy": "^4.5.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.10",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "less": "^4.1.2",
    "lint-staged": "^12.3.8",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.4.3",
    "prettier": "^2.6.2",
    "ramda": "^0.28.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-test-renderer": "^18.0.0",
    "rxjs": "^7.5.5",
    "snowpack": "^3.8.8",
    "standard-version": "^9.3.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "vite": "^2.6.14",
    "vite-aliases": "^0.9.2",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-react-refresh": "^2.3.8",
    "@snowpack/plugin-typescript": "^1.1.1",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.29",
    "@storybook/node-logger": "^6.4.22",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/ramda": "^0.28.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/compiler-sfc": "^3.0.4"
  }
}
